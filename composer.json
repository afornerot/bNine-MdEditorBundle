    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": [
        "symfony",
        "bundle",
        "markdown",
        "editor",
        "entity"
    ],
    "authors": [
        {
            "name": "afornerot",
            "email": "arno.nanor@gmail.com"
        }
